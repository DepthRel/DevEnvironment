<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>type.TryParse</Title>
      <Author>DepthRel</Author>
      <Description>Call insert of type.TryParse(input, out var value)</Description>
      <Shortcut>tp</Shortcut>
    </Header>
    <Snippet>
      <Code Language="CSharp">
        <![CDATA[$type$.TryParse($input$, out var $number$)$end$]]>
      </Code>
      <Declarations>
        <Literal>
          <ID>type</ID>
          <ToolTip>the type of parse</ToolTip>
          <Default>long</Default>
        </Literal>
        <Literal>
          <ID>number</ID>
          <ToolTip>result variable name</ToolTip>
          <Default>number</Default>
        </Literal>
        <Literal>
          <ID>input</ID>
          <ToolTip>Input string containing a number</ToolTip>
          <Default>Console.ReadLine()</Default>
        </Literal>
      </Declarations>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>