<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>int.TryParse</Title>
      <Author>DepthRel</Author>
      <Description>Call insert of int.TryParse(input, out var value)</Description>
      <Shortcut>itp</Shortcut>
    </Header>
    <Snippet>
      <Code Language="CSharp">
        <![CDATA[int.TryParse($Input$, out var $Number$)$end$]]>
      </Code>
      <Declarations>
        <Literal>
          <ID>Number</ID>
          <ToolTip>Int variable name</ToolTip>
          <Default>number</Default>
        </Literal>
        <Literal>
          <ID>Input</ID>
          <ToolTip>Input string containing a number</ToolTip>
          <Default>Console.ReadLine()</Default>
        </Literal>
      </Declarations>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>