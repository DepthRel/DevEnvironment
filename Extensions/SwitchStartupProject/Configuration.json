/* Docs: https://heptapod.host/thirteen/switchstartupproject/blob/branch/current/Configuration.md */
{
    /*  Create an item in the dropdown list for each project in the solution?  */
	"ListAllProjects": true,
	"Version": 3,
	"MultiProjectConfigurations": {
		"%Config name%": {
			"Projects": {
				"FirstProjectNameInSolution": {
					"CommandLineArguments": "%101%",
					"WorkingDirectory": "",
					"SolutionConfiguration": "Debug",
					"SolutionPlatform": "x64"
				},
				"SecondProjectNameInSolution": {
					"CommandLineArguments": "%Arg%",
					"WorkingDirectory": "",
					"SolutionConfiguration": "Debug",
					"SolutionPlatform": "x64"
				}
			}
		}
	}
}